{
    "name": "ecoughtry/nova-permissions",
    "description": "Laravel Nova 4 Grouped Permissions",
    "version": "1.0.2",
    "keywords": [ "laravel", "nova", "permission", "role" ],
    "license": "MIT",
    "prefer-stable": true,
    "repositories": [
      {
          "type": "composer",
          "url": "https://nova.laravel.com"
      },
        {
            "type": "vcs",
            "url": "https://github.com/TicketSnapGroup/laravel-permission"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0",
        "ticketsnapgroup/laravel-permission": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Sereny\\NovaPermissions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sereny\\NovaPermissions\\ToolServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
